{
  "name": "stylelint-diff-fixer",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "stylelint",
    "git",
    "diff",
    "css",
    "scss",
    "fix"
  ],
  "author": {
    "name": "Oteto",
    "url": "https://pote-chil.com/profile"
  },
  "license": "MIT",
  "files": [
    "./dist/"
  ],
  "bin": {
    "diff-lint": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:reset": "rm -rf ./dist && tsc",
    "prepare": "npm i typescript --save-dev && npm run build:reset",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "eslint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint": "npm run prettier && npm run eslint"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "commander": "^11.1.0",
    "fs": "^0.0.1-security",
    "postcss": "^8.4.31",
    "postcss-scss": "^4.0.9",
    "stylelint": "^15.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.9.0",
    "@types/postcss-js": "^4.0.3",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
